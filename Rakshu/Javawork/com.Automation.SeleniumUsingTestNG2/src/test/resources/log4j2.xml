<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
    
        <Console name="STDOUT"
                 target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
       
        
    </Appenders>
    
    <Loggers>
        <Root level="debug">
            <AppenderRef ref= "FILEOUT" level ="info"/>
            <AppenderRef ref= "STDOUT" level ="error"/>
        </Root>
    </Loggers>
</Configuration>